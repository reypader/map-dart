<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <module>photo-module</module>
    <version>1</version>
    <threadsafe>true</threadsafe>

    <static-files>
        <include path="/css/**"/>
        <include path="/fonts/**"/>
        <include path="/images/**"/>
        <include path="/lang/**"/>
        <include path="/lib/**"/>
        <include path="*.html"/>
        <include path="*.js"/>
    </static-files>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
</appengine-web-app>
